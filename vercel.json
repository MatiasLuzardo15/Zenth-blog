{
    "rewrites": [
        {
            "source": "/app",
            "destination": "https://zenth-task.vercel.app/index.html"
        },
        {
            "source": "/app/:path*",
            "destination": "https://zenth-task.vercel.app/:path*"
        },
        {
            "source": "/assets/:path*",
            "destination": "https://zenth-task.vercel.app/assets/:path*"
        },
        {
            "source": "/pandas/:path*",
            "destination": "https://zenth-task.vercel.app/pandas/:path*"
        },
        {
            "source": "/manifest.json",
            "destination": "https://zenth-task.vercel.app/manifest.json"
        },
        {
            "source": "/sw.js",
            "destination": "https://zenth-task.vercel.app/sw.js"
        },
        {
            "source": "/firebase-messaging-sw.js",
            "destination": "https://zenth-task.vercel.app/firebase-messaging-sw.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}